<div class="container-fluid">
	<div class="row-fluid">
		<div class="col-md-3">
			<div class="panel panel-primary">
					<div class="panel-heading">
						<div class="panel-title">
								Projects
						</div>
					</div>

					<div class="panel-body">
						{{#each item in content}}
							<div>
							{{#link-to 'projects.project-id' item.id}}
								{{item.name}}
							{{/link-to}}
							</div>
						{{/each}}
					</div>
			</div>
		</div>
		<div class="col-md-8">
			{{outlet}}
		</div>
	</div>
</div>