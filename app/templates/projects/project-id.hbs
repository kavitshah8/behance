<div class="project-header">
	<h3>{{content.name}}</h3>
	<small>
		{{#each item in content.fields}}
			{{item}}
		{{/each}}
	</small>
</div>

<div class="col-md-6">
	<img class="img-responsive" {{bind-attr src="content.covers.404"}} >
</div>

<div class="col-md-4 col-md-offset-2">
	<p class="text-uppercase">Project By</p>
	{{#each item in content.owners}}
		<div>{{item.first_name}} {{item.last_name}}</div>
	{{/each}}
</div>
{{outlet}}